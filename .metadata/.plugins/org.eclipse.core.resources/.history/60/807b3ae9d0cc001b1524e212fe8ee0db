package dto;

public class MemberDTO {
	private long no;
	private String id;
	private String name;
	private String grade;
	private int point;
	
	public MemberDTO() {}
	public MemberDTO(long no, String id, String name, String grade, int point) {
		super();
		this.no = no;
		this.id = id;
		this.name = name;
		this.grade = grade;
		this.point = point;
	}
	
	
}
